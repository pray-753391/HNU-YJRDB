Vector judge():
{
  Vector v=new Vector();
  Token t;
}
{
    t = <ICONSTANT>
    {v.addElement(Integer.parseInt(t.image));}
    (
      (          <ADD>{v.addElement("+");}
      	| < MINUS >{v.addElement("-");}
        | < MULTIPLY >{v.addElement("*");}
       	| < DIVIDE >{v.addElement("/");}
      )
