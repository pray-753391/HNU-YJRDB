public class yjrsql{  public static void main(String args []) throws ParseException  {    yjrsql parser = new yjrsql(System.in);
    int j=0;
    Vector v=new Vector();
    try
    { 
    	v=parser.judge();
    	j=1;
    }
    catch (Exception e)
      {
//        System.out.println(e.getMessage());
      }

	 if(0==j)
